{
  "setupFilesAfterEnv": ["./setupTests.js"],
  "collectCoverage": true,
  "testEnvironment": "jsdom",
  "transformIgnorePatterns": ["/node_modules/(?!(next-mdx-remote))"],
  "transform": {
    "\\.m?[jt]sx?$": [
      "esbuild-jest",
      {
        "sourcemap": true
      }
    ]
  },
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "mjs"],
  "verbose": true
}

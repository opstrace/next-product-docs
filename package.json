{
  "name": "@zentered/next-product-docs",
  "version": "0.0.127",
  "description": "React/Next.js Render Remote Content",
  "keywords": [
    "nextjs",
    "documentation"
  ],
  "bugs": {
    "url": "https://github.com/opstrace/next-product-docs/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/opstrace/next-product-docs.git"
  },
  "license": "Apache-2.0",
  "author": "Opstrace, Inc.",
  "contributors": [
    "Patrick Heneise (https://github.com/PatrickHeneise)",
    "Moritz Sattler (https://github.com/MoSattler)",
    "Christian Mejlak (https://github.com/C-Mejlak)",
    "Alexander Khvostov (https://github.com/alexanderKhvostov)"
  ],
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./serialize": "./serialize.js"
  },
  "browser": {
    "fs": false,
    "child_process": false
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "postinstall": "husky install",
    "lint": "eslint --fix . --ext .js --ext .jsx --ext md --ext mdx",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "style": "prettier --write \"./**/*.{js,jsx,json}\"",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "watch": "watch 'npm run build' ./src"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,json,jsx,md,mdx}": [
      "prettier --write"
    ],
    "*.{js,jsx,md,mdx}": [
      "eslint --cache --fix"
    ]
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "dependencies": {
    "@next/mdx": "^12.2.2",
    "github-slugger": "^1.4.0",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.21",
    "marked": "^4.0.18",
    "mdast": "^3.0.0",
    "next-mdx-remote": "^4.0.3",
    "prism-react-renderer": "1.3.5",
    "react-click-away-listener": "2.2.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-instantsearch-dom": "^6.30.2",
    "react-markdown": "^8.0.3",
    "react-scroll": "^1.8.7",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-sanitize": "^5.0.1",
    "rehype-slug": "^5.0.1",
    "remark-external-links": "^9.0.1",
    "remark-gfm": "^3.0.1",
    "remark-heading-id": "^1.0.0",
    "remark-inline-links": "^6.0.1",
    "strip-indent": "^4.0.0",
    "unist-util-visit": "^4.1.0",
    "vfile": "^5.3.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@mdx-js/runtime": "^1.6.22",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.3.0",
    "esbuild": "^0.14.49",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-mdx": "^2.0.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "next": "^12.2.2",
    "nock": "^13.2.9",
    "node-fetch": "~3.2.9",
    "pinst": "^3.0.0",
    "pkg-dir": "^6.0.1",
    "prettier": "^2.7.1",
    "react": "*",
    "react-dom": "*",
    "rollup": "^2.77.0",
    "watch": "^1.0.2"
  },
  "peerDependencies": {
    "next": ">11",
    "react": ">16",
    "react-dom": ">16"
  },
  "publishConfig": {
    "access": "public"
  }
}
